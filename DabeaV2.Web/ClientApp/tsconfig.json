{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@services/*": [ "./src/app/shared/services/*" ],
      "@guards/*": [ "src/app/shared/helper/*" ],
      "@interceptors/*": [ "./src/app/shared/interceptors/*" ],
      "@sharedModules/*": [ "./src/app/shared/modules/*" ],
      "@sharedComponents/*": [ "./src/app/shared/components/*" ]
    }
  }
  //"angularCompilerOptions": {
  //  "fullTemplateTypeCheck": true,
  //  "strictInjectionParameters": true
  //},
  //"include": [
  //  "src/**/*"
  //],
  //"exclude": [
  //  "node_modules/"
  //]
}
